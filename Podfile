# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'starter-app-template' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for starter-app-template

  pod 'Alamofire'
  pod 'AWSS3'

  pod 'ChameleonFramework/Swift', :git => 'https://github.com/ViccAlexander/Chameleon.git'

  pod 'FacebookCore'
  pod 'FacebookLogin'
  pod 'FacebookShare'
  pod 'Firebase', '~> 5.0'

  pod 'GoogleMaps'
  pod 'GooglePlaces’
  pod ‘GooglePlacePicker’

  pod 'IQKeyboardManagerSwift'

  pod 'ObjectMapper', '~> 3.1'

  pod 'SDWebImage'
  pod 'SideMenu'
  pod 'SVProgressHUD'

  pod 'TwitterKit'

  target 'starter-app-templateTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'starter-app-templateUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
